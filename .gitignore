# Ignore bundler config
/.bundle

# Ignore log files and temp files
/log/*
/tmp/*
!/log/.keep

# Ignore SQLite database files (if any)
*.sqlite3
*.sqlite3-journal

# Ignore database schema dump files (if any)
*.sql

# Ignore system-generated files
*.DS_Store

# Ignore the CSV files generated by the scraper
*.csv

# Ignore Ruby-specific files
/.ruby-version
/.ruby-gemset

# Ignore coverage report from SimpleCov or other tools
/coverage/

# Ignore the vendor directory (Bundler dependencies)
/vendor/bundle

# Ignore cache, pid, and lock files
/tmp/pids
/tmp/cache
/tmp/sockets

# Ignore byebug command history file
.byebug_history

# Ignore other temporary files like swap files
*.swp
*.swo
